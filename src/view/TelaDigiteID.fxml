<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="220.0" prefWidth="350.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.procurarObjeto.TelaDigiteIDController">
   <children>
      <Label layoutX="108.0" layoutY="38.0" text="Digite o ID do leitor:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="digitaID" layoutX="89.0" layoutY="80.0" promptText="Insira aqui...">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="botaoConfirmar" layoutX="228.0" layoutY="169.0" mnemonicParsing="false" onAction="#confirmarID" text="Confirmar" />
      <Button fx:id="botaoVoltar" layoutX="47.0" layoutY="169.0" mnemonicParsing="false" onAction="#voltar" prefHeight="25.0" prefWidth="69.0" text="Voltar" />
      <Label fx:id="mensagemErro" alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="126.0" prefHeight="20.0" prefWidth="281.0" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
